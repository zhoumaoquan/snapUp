page {
  padding-bottom: 200rpx;
  background-color: #F2F2F2;
}
.app-box-item {
  width: 100%;
  background-color: #FFFFFF;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  padding: 0 30rpx;
  height: 245rpx;
  margin-top: 20rpx;
}
.app-box-item .app-box-top {
  width: 100%;
  height: 155rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border-bottom: 1rpx solid #F2F2F2;
}
.app-box-item .app-box-top-icon {
  width: 60rpx;
  height: 60rpx;
  margin-right: 20rpx;
}
.app-box-item .app-box-top-info {
  height: 140rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: space-evenly;
  -webkit-justify-content: space-evenly;
          justify-content: space-evenly;
}
.app-box-item .app-box-top-info .app-address-not {
  font-size: 32rpx;
  font-weight: 400;
  color: #333;
}
.app-box-item .app-box-top-info .app-address-name {
  font-size: 30rpx;
  color: #333333;
  font-weight: 500;
}
.app-box-item .app-box-top-info .app-address-text {
  width: 570rpx;
  font-size: 26rpx;
  color: #999999;
  font-weight: 500;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.app-box-bot {
  height: 88rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.app-box-bot .app-box-btn {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.app-box-bot .app-box-btn .app-btn-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-left: 40rpx;
  font-size: 24rpx;
  line-height: 30rpx;
  color: #9E9E9E;
  font-weight: bold;
}
.app-box-bot .app-box-btn .app-btn-item image {
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.app-btn {
  width: 100%;
  height: 97rpx;
  background: -webkit-gradient(linear, left top, right top, from(#ED343D), to(#FF6638));
  background: -webkit-linear-gradient(left, #ED343D, #FF6638);
  background: linear-gradient(90deg, #ED343D, #FF6638);
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  color: #FFFFFF;
  font-size: 32rpx;
  font-weight: bold;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
}

