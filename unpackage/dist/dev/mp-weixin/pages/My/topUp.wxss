.app-box {
  margin-top: 170rpx;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  padding: 0 45rpx;
  background-color: #FFFFFF;
}
.app-box-title {
  font-size: 34rpx;
  color: #333333;
  font-weight: 400;
}
.app-box .app-input {
  width: 100%;
  height: 120rpx;
  border-bottom: 1rpx solid #F2F2F2;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-top: 10rpx;
}
.app-box .app-input .place {
  font-size: 50rpx;
  color: #606266;
  font-weight: 400;
}
.app-box .app-input input {
  width: 100%;
  height: 100%;
  font-size: 72rpx;
  font-weight: bold;
  color: #333333;
}
.app-box .app-input .text {
  font-size: 72rpx;
  font-family: SourceHanSansSC-Medium;
  color: #333333;
  margin-right: 10rpx;
}
.default-btn {
  font-weight: 400;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 108rpx;
  z-index: 99;
}

