<view data-event-opts="{{[['tap',[['jumpDatail',['$event']]]]]}}" class="goods-box" bindtap="__e"><image class="goods-box-img" src="{{produce.cover_path}}"></image><view class="goods-info"><view class="goods-info-title">{{produce.goods_name}}</view><block wx:if="{{produce.sold_out_status==1}}"><view class="goods-info-cont"><block wx:if="{{isAction}}"><view class="goods-info-cont-ing"><view class="goods-info-cont-l">{{"第"+produce.few_nums+"轮进行中"}}</view><view class="goods-info-cont-r">{{"已抢"+produce.rounds_nums+"份"}}</view></view></block><block wx:else><view class="goods-info-cont-time"><view class="goods-info-cont-l">开抢倒计时</view><view class="goods-info-cont-r"><u-count-down vue-id="800e09e0-1" timestamp="{{produce.time}}" show-days="{{true}}" separator-size="20" separator-color="#FF563E" font-size="20" color="#FF563E" data-event-opts="{{[['^end',[['downEnd']]]]}}" bind:end="__e" bind:__l="__l"></u-count-down></view></view></block></view></block><view class="goods-info-box"><view class="goods-info-box-l"><view class="goods-info-income"><text>{{"预估收益￥"+produce.goods_moeny}}</text></view><view class="goods-info-price"><text class="param">￥</text><text class="price">{{produce.price}}</text><text class="not">{{"￥"+produce.market_price}}</text></view></view><view class="goods-info-box-r"><block wx:if="{{isAction}}"><view class="goods-info-btn"><text>去抢购</text></view></block><block wx:else><block><block wx:if="{{produce.is_coll==0}}"><view data-event-opts="{{[['tap',[['follow',['$event']]]]]}}" class="goods-info-remind" catchtap="__e"><view class="goods-info-remind-title">关注提醒</view><view class="goods-info-remind-text">{{produce.member_coll+"人已关注"}}</view></view></block><block wx:if="{{produce.is_coll==1}}"><view data-event-opts="{{[['tap',[['notFollow',['$event']]]]]}}" class="{{['goods-info-remind',(produce.is_coll==1)?'active':'']}}" catchtap="__e"><view class="goods-info-remind-title">取消提醒</view><view class="goods-info-remind-text">{{produce.member_coll+"人已设置"}}</view></view></block></block></block></view></view></view><block wx:if="{{produce.sold_out_status==0}}"><view class="goods-mask"><image src="/static/image/shouqing.png"></image></view></block></view>