<view class="container u-skeleton"><scroll-view class="app-header" scroll-y="{{true}}" enable-flex="{{true}}"><block wx:for="{{detail.code_arr}}" wx:for-item="item" wx:for-index="index" wx:key="code"><view class="number-item"><text>抢货码：</text><view class="number-item-cont"><view class="number">{{item.code}}</view></view><block wx:if="{{item.status==1}}"><text>待公布</text></block><block wx:if="{{item.status==2}}"><text>已抢到</text></block><block wx:if="{{item.status==3}}"><text>未抢到</text></block></view></block></scroll-view><block wx:if="{{detail.status==3||detail.status==4}}"><view data-event-opts="{{[['tap',[['jumpLogistic',['$event']]]]]}}" class="app-address" bindtap="__e"><image class="app-address-icon" src="/static/icon/kuaid.png"></image><block wx:if="{{$root.g0.length>0}}"><view class="app-address-info"><view class="app-address-name"><text space="ensp">{{logistic.status}}</text></view><view class="app-address-text"><text space="ensp">{{logistic.time}}</text></view></view></block><u-icon vue-id="05d0cbce-1" name="arrow-right" size="24" color="#999" bind:__l="__l"></u-icon></view></block><view class="app-address"><block wx:if="{{detail.tag==0}}"><image class="app-address-icon" src="/static/icon/morendidian.png"></image></block><block wx:if="{{detail.tag==1}}"><image class="app-address-icon" src="/static/icon/jia.png"></image></block><block wx:if="{{detail.tag==2}}"><image class="app-address-icon" src="/static/icon/xiao.png"></image></block><block wx:if="{{detail.tag==3}}"><image class="app-address-icon" src="/static/icon/qi.png"></image></block><view class="app-address-info"><view class="app-address-name"><text space="ensp">{{detail.contacts+" "+detail.mobile}}</text></view><view class="app-address-text"><text space="ensp">{{detail.province_name+" "+detail.city_name+" "+detail.district_name+" "+detail.address}}</text></view></view></view><view class="app-title-one"><text>商品清单</text></view><good-order vue-id="05d0cbce-2" isDetail="{{true}}" produce="{{detail.goods_list[0]}}" bind:__l="__l"></good-order><view class="app-title-one mg-top"><text>订单信息</text></view><view class="order-info"><view class="order-info-item"><text class="title">订单编号</text><text class="text">{{detail.order_sn}}</text></view><view class="order-info-item"><text class="title">订单状态</text><text class="active">{{detail.status_name}}</text></view><view class="order-info-item"><text class="title">备注信息</text><text class="text">{{detail.remark}}</text></view></view><view class="app-title-one mg-top"><text>金额信息</text></view><view class="app-title-two"><text>商品总计</text><text>{{"￥"+detail.goods_amounts}}</text></view><view class="app-box"><view class="app-box-title"><text class="title">实付款:</text><text class="active">{{"￥"+detail.pay_amounts}}</text></view><view class="app-box-text"><text class="text">{{"抢购时间："+detail.create_time}}</text></view></view><block wx:if="{{detail.status==3}}"><view data-event-opts="{{[['tap',[['confirmReceipt',['$event']]]]]}}" class="order-tabbar" bindtap="__e"><view class="order-tabbar-item">确认收货</view></view></block><u-skeleton vue-id="05d0cbce-3" loading="{{loadSkeleton}}" bgColor="#F2F2F2" bind:__l="__l"></u-skeleton></view>